{
  "name": "esn-promise",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "J.K. Lover",
  "type": "module",
  "description": "Next Generation ECMAScript Promise Objects, AKA \"Post-ES6-era Promise\", is based on ECMAScript Language Specification(AKA ECMA-262 Standards) & Promises/A+ Specification.",
  "keywords": [
    "Promise",
    "ECMAScript Promise Objects",
    "ESNext Promise Objects",
    "esn-promise",
    "Promises/A+",
    "promises-aplus",
    "promises-aplus-tests"
  ],
  "files": [
    "dist",
    "index.d.ts"
  ],
  "main": "./dist/esn-promise.js",
  "module": "./dist/esn-promise.js",
  "types": "./index.d.ts",
  "exports": {
    "types": "./index.d.ts",
    "import": "./dist/esn-promise.js"
  },
  "scripts": {
    "start": "vitest --ui",
    "test": "vitest",
    "aplus": "tsx __tests__/adapter",
    "build": "tsc && vite build",
    "commit": "pnpm cz",
    "lint": "eslint --ext .ts {src,__tests__}/**.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "postinstall": "simple-git-hooks",
    "release": "standard-version"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitest/ui": "^0.28.4",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^13.1.1",
    "prettier": "^2.8.4",
    "promises-aplus-tests": "^2.1.2",
    "promises-es6-tests": "^0.5.0",
    "simple-git-hooks": "^2.8.1",
    "standard-version": "^9.5.0",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.4"
  }
}
